{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "provisioning-service",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "provisioning_service.main:app",
                "--host",
                "127.0.0.1",
                "--port",
                "8000",
                "--reload"
            ],
            "jinja": false,
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "python": "${command:python.interpreterPath}"
        }
    ]
}